    <div class="side_top_bar">
      <div class="user_name">
        <%= link_to current_user.name, edit_user_path(current_user)%>
      </div>
      <div class="chat_btn">
        <%= link_to "チャットを作成する", new_room_path %>
      </div>
    </div>
    <div class="side_bottom_bar">
      <% current_user.rooms.each do |room| %>
        <div class="chat_content">
          <div class="chat_room">
            <a href="#"><%= room.name %></a>
          </div>
        </div>
      <% end %>
    </div>